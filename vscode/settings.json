// "vim.debug.loggingLevelForAlert": "debug",
"vim.autoindent": true,
"vim.handleKeys": {
    "<C-d>": false,
    "<C-/>": false,
    "<C-z>": false,
    "<C-Z>": false,
    "<C-]>": false,
    "<C-[>": false,
    "u": false,
    "<C-y>": true,
    "Up": true,
    "Down": true,
    "Left": true,
    "Right": true,
    "<C-k>": false,
},
"vim.insertModeKeyBindingsNonRecursive": [
    {
        "before": ["<alt-k>"],
        "after": ["g", "k"]
    },
    {
        "before": ["<alt-j>"],
        "after": ["g", "j"]
    },
    // {
    //     "before": ["<leader>","c","i"],
    //     "commands": ["editor.action.commentLine"]
    // },
],
"vim.normalModeKeyBindingsNonRecursive": [
    {
        "before": ["<alt-k>"],
        "after": ["g", "k"]
    },
    {
        "before": ["<alt-j>"],
        "after": ["g", "j"]
    },
    // {
    //     "before": ["o"],
    //     "commands": ["editor.action.insertLineAfter", "extension.vim_insert"]
    // },
    // {
    //     "before": ["O"],
    //     "commands": ["editor.action.insertLineBefore", "extension.vim_insert"]
    // },
    {
        "before": ["<leader>","c","i"],
        "commands": ["editor.action.commentLine"]
    },
    {
        "before": ["K"],
        "after": ["1","0","-"]
    },
    {
        "before": ["J"],
        "after": ["1","0","+"]
    },
    {
        "before": ["u"],
        "commands": ["undo"]
    },
    {
        "before": ["<C-r>"],
        "commands": ["redo"]
    },
    // {
    //     "before": ["S"],
    //     "after": ["y", "s", "a", "W", "$"]
    // },
],
"vim.visualModeKeyBindingsNonRecursive": [
    {
        "before": ["k"],
        "after": ["g", "k"]
    },
    {
        "before": ["j"],
        "after": ["g", "j"]
    },
    {
        "before": ["K"],
        "after": ["1","0","-"]
    },
    {
        "before": ["J"],
        "after": ["1","0","+"]
    },
    {
        "before": [ "p"],
        "after": [ "p", "g", "v", "y" ]
    },
    {
        "before": ["u"],
        "commands": ["undo"]
    },
    {
        "before": ["<C-r>"],
        "commands": ["redo"]
    },
    {
        "before": ["<leader>","c","i"],
        "commands": ["editor.action.commentLine"]
    },
],
