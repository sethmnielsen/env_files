"python.terminal.activateEnvironment": false,
"zenMode.hideLineNumbers": false,
"editor.cursorSmoothCaretAnimation": true,
"editor.acceptSuggestionOnEnter": "off",
"activitusbar.activeColour": "#4989b6",
"activitusbar.toggleSidebar": false,
"activitusbar.searchViewInPanel": false,
"activitusbar.views": [
    {
        "name": "explorer",
        "codicon": "explorer-view-icon"
    },
    {
        "name": "search",
        "codicon": "search-view-icon"
    },
    {
        "name": "scm",
        "codicon": "source-control-view-icon"
    },
    {
        "name": "debug",
        "codicon": "run-view-icon"
    },
    {
        "name": "extensions",
        "codicon": "extensions-view-icon"
    },
    {
        "name": "extension.liveshare",
        "codicon": "live-share"
    },
    {
        "name": "extension.latex",
        "codicon": "whitespace"
    },
],
"macros.list": {
    "closePanelSidebar": [
        "workbench.action.toggleSidebarVisibility",
        "workbench.action.closePanel"
    ],
    "resizePaneRight": [
        "workbench.action.terminal.resizePaneRight"
    ],
    "resizePaneLeft": [
        "workbench.action.terminal.resizePaneLeft"
    ],
    "openTerminalToSide": [
        "workbench.action.positionPanelRight",
        {
            "command": "resizePaneRight",
            "args": {
                "times":4
            }
        },
        {
            "command": "resizePaneLeft",
            "args": {
                "times":4
            }
        }
    ],
    "positionPanelRightTerminalFocus": [
        "workbench.action.positionPanelRight",
        "workbench.action.focusActiveEditorGroup",
        "terminal.focus"
    ],
    "positionPanelBottomTerminalFocus": [
        "workbench.action.positionPanelBottom",
        "terminal.focus"
    ],
    "positionPanelRightEditorFocus": [
        "workbench.action.positionPanelRight",
        "workbench.action.focusActiveEditorGroup"
    ],
    "positionPanelBottomEditorFocus": [
        "workbench.action.positionPanelBottom",
        "workbench.action.focusActiveEditorGroup"
    ],
    "runAllAndRestartKernel": [
        "jupyter.restartkernel",
        "jupyter.runallcells",
    ],
    "runAboveAndRestartKernel": [
        "jupyter.restartkernel",
        "jupyter.runallcellsabove.palette",
    ],
    "runAboveAndDebugCurrentCell": [
        "jupyter.restartkernel",
        "jupyter.runallcellsabove.palette",
        "jupyter.debugcurrentcell.palette"
    ],
    "saveAndRunCode": [
        "workbench.action.files.save",
        "code-runner.run",
    ]
},
"scm.diffDecorationsGutterWidth": 1,
"explorer.confirmDragAndDrop": false,
"editor.suggestSelection": "first",
"explorer.confirmDelete": false,
"files.watcherExclude": {
    "/home/seth/.local/share/**": true,
    "/home/seth/Downloads/**": true
},
"workbench.list.automaticKeyboardNavigation": false,
"gitlens.codeLens.authors.enabled": false,
"gitlens.codeLens.recentChange.enabled": false,
"gitlens.currentLine.enabled": false,
"gitlens.advanced.messages": {
    "suppressGitDisabledWarning": true
},
"gitlens.statusBar.reduceFlicker": true,
"problems.decorations.enabled": true,
"todohighlight.keywords": [
    {
        "text": "HACK:",
        "color": "#000",
        "isWholeLine": false,
    },
    {
        "text": "NOTE:",
        "color": "turquoise",
        "backgroundColor": "none",
    },
    {
        "text": "TODO:",
        "color": "crimson",
        "backgroundColor": "none",
        // "border": "1px solid red",
        // "borderRadius": "2px", //NOTE: using borderRadius along with `border` or you will see nothing change
    }
],
"window.customMenuBarAltFocus": false,
"window.enableMenuBarMnemonics": false,
"editor.snippetSuggestions": "bottom",
"extensions.autoUpdate": false,
"editor.links": false,
"code-runner.executorMap": {
    "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && ./$fileNameWithoutExt",
    // "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt && ./$fileNameWithoutExt",
    "cpp": "g++ $fileName -o $fileNameWithoutExt && ./$fileNameWithoutExt",
    "python": "cd $dir && ipython $fileName",
    "shellscript": "bash",
    "rust": "cargo run"
},
"code-runner.runInTerminal": true,
"code-runner.enableAppInsights": false,
"code-runner.showRunCommandInExplorerContextMenu": false,
"code-runner.showRunCommandInEditorContextMenu": false,
"[latex]": {
    "editor.formatOnPaste": false,
    "editor.formatOnSave": false,
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 100,
    "editor.tabSize": 4,
    "editor.suggest.showSnippets": true,
    "editor.snippetSuggestions": "inline"
},
"latex-workshop.chktex.enabled": true,
"latex-workshop.chktex.args.root": [ "-wall", "-n1", "-n22", "-n30", "-n46", "-e16", "-q" ],
"latex-workshop.chktex.args.active": [ "-wall", "-n1", "-n9", "-n13", "-n17", "-n22", "-n30", "-n45", "-n46", "-e16", "-q" ],
"latex-workshop.chktex.run": "onType",
"latex-workshop.chktex.delay": 2000,
"latex-workshop.latex.autoBuild.run": "never",
"latex-workshop.view.pdf.viewer": "external",
"latex-workshop.latex.recipe.default": "first",
"latex-workshop.view.pdf.hand": true,
"latex-workshop.message.error.show": false,
"latex-workshop.message.warning.show": false,
"latex-workshop.view.pdf.backgroundColor": "#e6e6e6",
"latex-workshop.message.log.show": false,
"latex-workshop.latex.tools": [
    {
        "name": "lualatexmk",
        "command": "latexmk",
        "args": [
            "-synctex=1",
            "-interaction=nonstopmode",
            "-file-line-error",
            "-lualatex",
            "-outdir=%OUTDIR%",
            "%DOC%"
        ],
        "env": {}
    },
    {
        "name": "latexmk",
        "command": "latexmk",
        "args": [
            "-pdf",
            "-f",
            "%DOC%"
        ],
        "env": {}
    },
    {
        "name": "biber",
        "command": "biber",
        "args": [
            "%OUTDIR%/%DOCFILE%"
        ],
        "env": {}
    },
    {
        "name": "bibtex",
        "command": "bibtex",
        "args": [
            "%OUTDIR%/%DOCFILE%"
        ],
        "env": {}
    }
],
"latex-workshop.latex.recipes": [
    {
        "name": "lualatexmk",
        "tools": [
            "lualatexmk"
        ]
    },
    {
        "name": "latexmk",
        "tools": [
            "latexmk"
        ]
    },
    {
        "name": "biber ➞ lualatexmk",
        "tools": [
            "biber",
            "lualatexmk"
        ]
    },
    {
        "name": "pdflatex ➞ bibtex ➞ pdflatex × 2",
        "tools": [
            "pdflatex",
            "bibtex",
            "pdflatex",
            "pdflatex"
        ]
    }
],
"vim.vimrc.enable": true,
"vim.vimrc.path": "/home/seth/env/.vimrc",
"vim.useCtrlKeys": true,
"vim.smartcase": true,
"vim.ignorecase": true,
"vim.foldfix": true,
"vim.useSystemClipboard": true,
"vim.searchHighlightColor": "gray",
"vim.incsearch": true,
"vim.visualstar": true,
// "vim.debug.silent": false,
