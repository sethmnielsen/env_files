"vim.autoindent": true,
"vim.handleKeys": {
    "<C-d>": false,
    "<C-/>": false,
    "<C-z>": false,
    "<C-Z>": false,
    "<C-]>": false,
    "<C-[>": false,
    "<C-y>": true,
    "Up": true,
    "Down": true,
    "Left": true,
    "Right": true,
    "<C-k>": false,
    "<C-n>": false,
    "<C-m>": false,
},
"vim.normalModeKeyBindingsNonRecursive": [
    {
        "before": ["g","L"],
        "after": ["g", "$", "g", "e"]
    },
    {
        "before": ["u"],
        "commands": ["undo"]
    },
    {
        "before": ["<C-r>"],
        "commands": ["redo"]
    },
    {
        "before": ["<leader>","w"],
        "commands": ["workbench.action.files.save"]
    },
    {
        "before": ["<leader>","a"],
        "after": ["G","V","g","g"]
    },
],
"vim.visualModeKeyBindingsNonRecursive": [
    {
        "before": ["k"],
        "after": ["g", "k"]
    },
    {
        "before": ["j"],
        "after": ["g", "j"]
    },
    {
        "before": ["K"],
        "after": ["1","0","k"]
    },
    {
        "before": ["J"],
        "after": ["1","0","j"]
    },
    {
        "before": [ "p"],
        "after": [ "p", "g", "v", "y" ]
    },
    {
        "before": ["u"],
        "commands": ["undo"]
    },
    {
        "before": ["<C-r>"],
        "commands": ["redo"]
    },
    {
        "before": ["<leader>","c","i"],
        "commands": ["editor.action.commentLine"]
    },
    {
        "before": ["<leader>","p"],
        "after": ["p","G"]
    },
    {
        "before": ["<leader>","y"],
        "after": ["y","G"]
    },
],
"editor.smoothScrolling": true,
