{
"workbench.startupEditor": "none",
"editor.fontLigatures": false,
"terminal.integrated.fontFamily": "'FantasqueSansMono Nerd Font Mono', 'DejaVuSansMono Nerd Font Mono'",
"editor.wordWrapColumn": 100,
"editor.rulers": [100],
"search.useReplacePreview": false,
"C_Cpp.default.cppStandard": "c++14",
"files.exclude": {
    "**/*__pycache__": true,
    "**/*.~*": true,
    "**/*.catkin_workspace": true,
    "**/*.github/": true,
    "**/*.gitignore": true,
    "**/*.gitmodules": true,
    "**/*.ipynb_checkpoints": true,
    "**/*.m": true,
    "**/*.mypy_cache": true,
    "**/*.o": true,
    "**/*.ods": true,
    "**/*.odt": true,
    "**/*.pyc": true,
    "**/*.rst": true,
    "**/*.so": true,
    "**/*.swp": true,
    "**/*.gz": true,
    "**/*.zip": true,
    "**/*CMakeLists.txt.user": true,
    "**/*docs": true,
    "**/Cargo.lock": true,
    "**/build": true,
    "**/devel": true,
    "**/target/": true,
    "**/*.pytest_cache/": true,
    "**/*.vcxproj": true,
    "**/*.vcxproj.filters": true,
    "**/*.bat": true,
    "**/*.aux": true,
    "**/*.bbl": true,
    "**/*.blg": true,
    "**/*.fdb_latexmk": true,
    "**/*.fls": true,
    "**/*.lof": true,
    "**/*.lot": true,
    "**/*.toc": true,
    "**/*.out": true,
    "**/*.lb": true,
    "**/*.gitkeep": true,
},
"files.associations": {
    "*.*rc": "shellscript",
    "*.launch": "xml",
    "CMakeLists.txt": "cmake",
    "<ipython-input*": "python",
    "*.bbl": "tex",
    "*.metadata": "json",
    "*.vim": "shellscript"
},
"explorer.autoReveal": false,
"explorer.openEditors.visible": 0,
"editor.minimap.enabled": false,
"atomKeymap.promptV3Features": true,
"editor.multiCursorModifier": "alt",
